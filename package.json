{
    "name": "aw-watcher-vscode",
    "displayName": "aw-watcher-vscode",
    "description": "vs-code extension for the open source tracking tool ActivityWatch",
    "version": "0.3.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/Otto-AA/aw-watcher-vscode"
    },
    "publisher": "lindraupe",
    "author": {
        "name": "A_A",
        "url": "http://aapianomusic.bplaced.net"
    },
    "bugs": {
        "url": "https://github.com/Otto-AA/aw-watcher-vscode/issues"
    },
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "multi-root ready"
    ],
    "icon": "logo.png",
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.reload",
                "title": "Reload ActivityWatch"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "aw-watcher-vscode-extension configuration",
            "properties": {
                "aw-watcher-vscode.maxHeartbeatsPerSec": {
                    "type": "number",
                    "default": 1,
                    "description": "Controls the maximum number of hearbeats sent per second."
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "test:mocha": "npm run compile && node ./node_modules/mocha/bin/mocha ./out/test/extension.test.js"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "mocha": "^5.2.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {
        "axios": "^0.18.0"
    }
}